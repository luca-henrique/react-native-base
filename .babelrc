{
  "presets": ["module:metro-react-native-babel-preset"],
  "plugins": [
    [
      "babel-plugin-inline-import",
      {
        "extensions": [".svg"]
      }
    ],
    [
      "module-resolver",
      {
        "root": "./src",
        "alias": {
          "components": "./components/*"
        },
        "cwd": "packagejson"
      }
    ]
  ]
}
